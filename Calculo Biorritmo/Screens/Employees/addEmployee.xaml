<Window x:Class="Calculo_Biorritmo.Screens.Employees.addEmployee"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculo_Biorritmo.Screens.Employees"
        mc:Ignorable="d"
        Title="addEmployee" Height="450" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="AUTO"/>
            <RowDefinition/>
            <RowDefinition Height="AUTO"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Label Content="Aqui va tu contexto"/>
        </Grid>
        <Grid Grid.Row="1">
            <Label Content="Agregar Formulario aqui"/>
        </Grid>
        <Grid Grid.Row="2">
            <Button Name="btnRegresar" Content="Regresar" HorizontalAlignment="Right" Width="200" Height="50" VerticalAlignment="Bottom" Margin="20" Click="btnRegresar_Click"/>
        </Grid>
        
    </Grid>
</Window>
